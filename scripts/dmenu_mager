DMENU="dmenu -p Mager $@"
choice=$(echo -e "anime\naria2\nboottester\nkill\nmonitor\nmpdmenu\npass\npower\nqutebrowser\nrecord\nswitcher\ntodo\nudiskie\nwifi\nwindow\nytview" | $DMENU)

case "$choice" in
anime) dmenu_anime "$@";;
aria2) dmenu_aria2 "$@" ;;
boottester) dmenu_boottester "$@" ;;
kill) dmenu_kill "$@" ;;
monitor) dmenu_monitor "$@" ;;
mpdmenu) dmenu_mpdmenu "$@" ;;
pass) dmenu_pass -p "Pass Man" "$@" ;;
power) dmenu_power "$@" ;;
qutebrowser) dmenu_qutebrowser "$@" ;;
record) dmenu_record "$@" ;;
switcher) dmenu_switcher "$@" ;;
todo) dmenu_todo "$@" ;;
udiskie) dmenu_udiskie "$@" ;;
wifi) dmenu_wifi "$@" ;;
window) dmenu_window "$@" ;;
ytview) dmenu_ytview "$@" ;;
esac
